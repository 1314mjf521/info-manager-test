# 服务器配置
IMS_SERVER_PORT=8080
IMS_SERVER_MODE=debug

# 数据库配置
IMS_DATABASE_TYPE=sqlite
IMS_DATABASE_HOST=localhost
IMS_DATABASE_PORT=5432
IMS_DATABASE_USERNAME=
IMS_DATABASE_PASSWORD=
IMS_DATABASE_DATABASE=./data/info_system.db
IMS_DATABASE_SSL_MODE=disable

# Redis配置
